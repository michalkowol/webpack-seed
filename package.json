{
  "name": "webpack-seed",
  "version": "1.0.0",
  "description": "webpack-seed",
  "repository": {
    "type": "git",
    "url": "https://github.com/michalkowol/webpack-seed"
  },
  "license": "ISC",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
