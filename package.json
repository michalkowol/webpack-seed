    "babel-core": "6.22.x",
    "babel-loader": "6.2.x",
    "babel-preset-es2015": "6.22.x",
    "babel-preset-react": "6.22.x",
    "babel-preset-stage-2": "6.22.x",
    "react-hot-loader": "3.0.0-beta.6",
    "babel-eslint": "7.1.x",
